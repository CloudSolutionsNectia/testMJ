        <externalId>false</externalId>
        <formula>IF( 
( CheckOutDate__c - CreatedDate)&gt; 0 , 
(FLOOR( CheckOutDate__c- CreatedDate) *24*60)+ 
(FLOOR( MOD( (CheckOutDate__c - CreatedDate) * 24, 24 ) ) *60)+ 
(ROUND( MOD( (CheckOutDate__c - CreatedDate) * 24 * 60, 60 ), 0 ) ),0 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Duración de visita</label>
