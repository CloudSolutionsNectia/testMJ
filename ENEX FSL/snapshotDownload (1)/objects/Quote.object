{! requireScript(&apos;/soap/ajax/20.0/apex.js&apos;) }

var quoteId = &quot;{!Quote.Id}&quot;;
var connection = sforce.connection;

function updateQuoteStatus(status) {
  var quote = connection.query(&apos;SELECT Id, Status FROM Quote WHERE ID = \&apos;&apos; + quoteId + &apos;\&apos;&apos;).getArray(&apos;records&apos;)[0];
  quote.Status = status;
  var results = sforce.connection.update([quote]);
  return reportMessages(results, 1);
}

function reportMessages(results, itemsLength) {
  if (results.length != itemsLength) {
    alert(&apos;Problemas al actualizar los registros!&apos;);
    return false;
  }
  var message = &apos;&apos;;
  for (var i=0; i &lt; results.length; i++){
    if (!results[i].getBoolean(&apos;success&apos;)){
      message += results[i];
    }
  }
  if (message != &apos;&apos;) {
    alert(&apos;ERRORES:&apos; + message);
    return false;
  }
  return true;
}

function requestAproval() {
  var script = &apos;Approval.ProcessSubmitRequest req = new Approval.ProcessSubmitRequest();&apos;;
  script += &apos;req.setComments(\&apos;Cotización enviada a aprobación. Favor revisar detalles para aprobar.\&apos;);&apos;;
  script += &apos;req.setObjectId(\&apos;&apos; + quoteId + &apos;\&apos;);&apos;;
  script += &apos;Approval.ProcessResult result = Approval.process(req);&apos;;
  var result = sforce.apex.executeAnonymous(script);
  if (!eval(result.success)) {
    if (result.exceptionMessage.search(&quot;NO_APPLICABLE_PROCESS&quot;) != -1)
     alert(&quot;No existe proceso de aprobación que calce con la información de esta cotización.&quot;);
    else if (result.exceptionMessage.search(&quot;MANAGER_NOT_DEFINED&quot;) != -1)
     alert(&quot;No se ha podido enviar al proceso de aprobación debido a que el aprobador no está definido&quot;);
    else
     alert(result.exceptionMessage);
    return false;
  }
  return true;
}

function process() {
  if (&apos;{!Quote.Status}&apos; == &apos;En revisión&apos;) {
    alert(&apos;Esta cotización ya se encuentra en proceso de aprobación. Cuando este proceso termine puede solicitar nuevas aprobaciones.&apos;);
    return;
  }
  if (requestAproval()){
    alert(&quot;Solicitud de aprobación enviada.&quot;);
    updateQuoteStatus(&apos;En revisión&apos;);
    window.location.reload();
  }
}

